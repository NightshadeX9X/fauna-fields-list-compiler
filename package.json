    "apollo-server": "^2.9.7",
    "dotenv": "^8.2.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "graphql": "^14.5.8",
    "nodemon": "^1.19.4",
